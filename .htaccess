# Forzar el favicon personalizado
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} !^/favicon\.ico$ [NC]
    RewriteRule ^favicon\.ico$ /Imagenes/logo_back_dark_cut.png [L]
</IfModule>

# Configuración de caché para el favicon
<FilesMatch "\.(ico|png)$">
    Header set Cache-Control "public, max-age=31536000"
</FilesMatch>

